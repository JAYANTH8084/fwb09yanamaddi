extends layout

block content
  h1= title
  br
  - var image_names = ["baseball.jpg","basketball.jpg","football1.png","football2.jpg","tennis.jpeg"];
  - for(let i=0; i<3; i++){
    - let number = Math.floor(Math.random()*5);
    img(src = `/images/${image_names[number]}` alt = `Image No ${image_names[number]}`)
    -}